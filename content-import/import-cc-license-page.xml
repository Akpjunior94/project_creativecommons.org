<!-- /wp:image -->

<!-- wp:paragraph -->
<p><strong>CC BY</strong></p>
<!-- /wp:paragraph --></div>
<!-- /wp:column --></div>
<!-- /wp:columns -->

<!-- wp:paragraph {"fontSize":"medium"} -->
<p class="has-medium-font-size"><strong>Attribution</strong></p>
<!-- /wp:paragraph -->

<!-- wp:buttons {"contentJustification":"right","align":"full"} -->
<div class="wp-block-buttons alignfull is-content-justification-right"><!-- wp:button {"backgroundColor":"white","textColor":"tomato","fontSize":"small"} -->
